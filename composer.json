{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "crimsonstrife/streamer.live",
    "type": "application",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "a21ns1g4ts/filament-short-url": "^0.1.5",
        "asosick/filament-layout-manager": "^1.1",
        "awcodes/filament-tiptap-editor": "^3.5",
        "bepsvpt/secure-headers": "^9.0",
        "blade-ui-kit/blade-ui-kit": "^0.7.0",
        "calebporzio/sushi": "^2.5",
        "cocur/slugify": "^4.6",
        "doctrine/dbal": "^4.2",
        "filament/actions": "^3.2",
        "filament/filament": "^3.2",
        "filament/forms": "^3.2",
        "filament/infolists": "^3.2",
        "filament/spatie-laravel-media-library-plugin": "^3.2",
        "filament/spatie-laravel-settings-plugin": "^3.2",
        "filament/widgets": "^3.2",
        "indracollective/laravel-revisor-filament": "^0.0.1",
        "kalnoy/nestedset": "^6.0",
        "laravel/framework": "^11.31",
        "laravel/jetstream": "^5.3",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.12",
        "laravel/socialite": "^5.17",
        "laravel/tinker": "^2.9",
        "laravel/ui": "^4.6",
        "livewire/livewire": "^3.0",
        "nesbot/carbon": "^3.8",
        "patreon/patreon": "^1.0",
        "ralphjsmit/laravel-seo": "^1.6",
        "ramsey/uuid": "^4.7",
        "romanzipp/laravel-twitch": "^4.8",
        "spatie/enum": "^3.13",
        "spatie/invade": "^2.1",
        "spatie/laravel-error-solutions": "^1.0",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-navigation": "^1.3",
        "spatie/laravel-permission": "^6.10",
        "spatie/laravel-settings": "^3.4",
        "spatie/laravel-sluggable": "^3.7",
        "spatie/schema-org": "^3.23",
        "stephenjude/filament-debugger": "^4.0",
        "symfony/polyfill-php83": "^1.31",
        "theiconic/name-parser": "^1.2",
        "tinymce/tinymce": "^7.6",
        "xetaio/xetaravel-mentions": "^11.0",
        "z3d0x/filament-fabricator": "^2.4"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.14",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/horizon": "^5.29",
        "laravel/pail": "^1.1",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.26",
        "laravel/telescope": "^5.2",
        "laravel/tinker": "^2.10",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.5",
        "orchestra/testbench": "^9.6",
        "pestphp/pest": "v3.7.4",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.4",
        "phpstan/phpstan": "^2.0.2",
        "phpunit/phpunit": "^11.4.3",
        "roave/security-advisories": "dev-latest",
        "spatie/laravel-blade-comments": "^1.3",
        "spatie/laravel-ignition": "^2.8",
        "spatie/laravel-sql-commenter": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan filament:upgrade",
            "@php artisan tinymce:publish --ansi",
            "npm update",
            "@php artisan jquery:publish --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope",
                "laravel/tinker",
                "laravel/horizon"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
