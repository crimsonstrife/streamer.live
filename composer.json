{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": ["laravel", "framework"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "a21ns1g4ts/filament-short-url": "^0.1.5",
        "asmit/filament-mention": "^1.0",
        "asosick/filament-layout-manager": "^1.1",
        "bezhansalleh/filament-shield": "^3.3",
        "filament/filament": "^3.3",
        "filament/spatie-laravel-media-library-plugin": "^3.3",
        "filament/spatie-laravel-settings-plugin": "^3.3",
        "filament/spatie-laravel-tags-plugin": "^3.3",
        "guava/calendar": "^1.13",
        "guava/tutorials": "^1.1",
        "ibrahim-bougaoua/filaprogress": "^0.1.0",
        "indracollective/laravel-revisor-filament": "^0.0.1",
        "joaopaulolndev/filament-edit-profile": "^1.0",
        "laravel/framework": "^11.0",
        "laravel/horizon": "^5.30",
        "laravel/jetstream": "^5.3",
        "laravel/pennant": "^1.16",
        "laravel/pulse": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/telescope": "^5.5",
        "laravel/tinker": "^2.10.1",
        "livewire/livewire": "^3.0",
        "parallax/filament-comments": "^1.4",
        "pxlrbt/filament-activity-log": "^1.1",
        "schmeits/filament-character-counter": "^1.3",
        "shipu/web-installer": "^1.0",
        "shuvroroy/filament-spatie-laravel-health": "^2.3",
        "solution-forest/filament-access-management": "^2.2",
        "spatie/eloquent-sortable": "^4.4",
        "spatie/error-solutions": "^1.1",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-honeypot": "^4.5",
        "spatie/laravel-ignition": "^2.9",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-permission": "^6.15",
        "spatie/laravel-searchable": "^1.13",
        "spatie/laravel-settings": "^3.4",
        "spatie/laravel-sitemap": "^7.3",
        "spatie/laravel-sluggable": "^3.7",
        "spatie/laravel-tags": "^4.9",
        "stephenjude/filament-blog": "^4.0",
        "stephenjude/filament-debugger": "^4.0",
        "stephenjude/filament-feature-flags": "^2.1",
        "stephenjude/filament-jetstream": "^0.0.15",
        "tapp/filament-auditing": "^3.0",
        "z3d0x/filament-fabricator": "^2.4"
    },
    "require-dev": {
        "fakerphp/faker": "^1.23",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
