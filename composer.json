{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "crimsonstrife/streamer",
    "type": "application",
    "description": "A Laravel-based webapp for hosting content creator fan sites, aimed a streamers and their community building. Built with Laravel, Livewire, and Filament.",
    "keywords": [
        "laravel",
        "livewire",
        "filament",
        "streamer",
        "streaming",
        "fan site",
        "community",
        "content creator"
    ],
    "authors": [
        {
            "name": "Patrick Barnhardt (crimsonstrife)",
            "email": "contact@patrickbarhardt.info"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.3",
        "a21ns1g4ts/filament-short-url": "^0.1.5",
        "asmit/filament-mention": "^1.0",
        "asosick/filament-layout-manager": "^1.1",
        "bepsvpt/secure-headers": "*",
        "blade-ui-kit/blade-icons": "^1.8",
        "blade-ui-kit/blade-ui-kit": "^0.7.0",
        "brickx/maintenance-switch": "^1.0",
        "calebporzio/sushi": "^2.5",
        "cocur/slugify": "^4.6",
        "commerceguys/addressing": "^2.2",
        "commerceguys/intl": "^2.0",
        "doctrine/dbal": "^4.2",
        "drewm/mailchimp-api": "^2.5",
        "enshrined/svg-sanitize": "^0.21.0",
        "filament/actions": "^3.3",
        "filament/filament": "^3.3",
        "filament/forms": "^3.3",
        "filament/infolists": "^3.3",
        "filament/notifications": "^3.3",
        "filament/spatie-laravel-media-library-plugin": "^3.3",
        "filament/spatie-laravel-settings-plugin": "*",
        "filament/spatie-laravel-tags-plugin": "^3.3",
        "filament/tables": "^3.3",
        "filament/widgets": "^3.3",
        "gerenuk/filament-banhammer": "^1.1",
        "guava/calendar": "^1.14",
        "guava/tutorials": "^1.1",
        "guzzlehttp/guzzle": "^7.9",
        "ibrahim-bougaoua/filaprogress": "^0.1.0",
        "illuminate/support": "*",
        "indracollective/laravel-revisor-filament": "^0.0.2",
        "joaopaulolndev/filament-edit-profile": "^1.0",
        "kalnoy/nestedset": "^6.0",
        "lakm/laravel-comments": "^2.0",
        "laravel/fortify": "^1.24",
        "laravel/framework": "^11.0",
        "laravel/horizon": "^5.30",
        "laravel/jetstream": "^5.3",
        "laravel/pennant": "^1.16",
        "laravel/pulse": "^1.4",
        "laravel/sanctum": "^4.0",
        "laravel/scout": "^10.11",
        "laravel/socialite": "^5.21",
        "laravel/telescope": "^5.5",
        "laravel/tinker": "^2.10.1",
        "laravel/ui": "^4.5",
        "livewire/livewire": "^3.0",
        "mchev/banhammer": "^2.3",
        "mews/purifier": "^3.4",
        "nesbot/carbon": "^3.8",
        "nickurt/laravel-akismet": "^2.0",
        "nickurt/laravel-stopforumspam": "^2.0",
        "omaressaouaf/laravel-id-generator": "^1.2",
        "parallax/filament-comments": "^1.4",
        "pxlrbt/filament-activity-log": "^1.1",
        "ramsey/uuid": "^4.7",
        "schmeits/filament-character-counter": "^1.3",
        "shieldon/event-dispatcher": "^1.0",
        "shieldon/messenger": "^1.6",
        "shieldon/shieldon": "^2.1",
        "shieldon/web-security": "*",
        "shipu/web-installer": "^1.0",
        "shuvroroy/filament-spatie-laravel-health": "^2.3",
        "socialiteproviders/discord": "*",
        "socialiteproviders/google": "^4.1",
        "socialiteproviders/microsoft": "^4.5",
        "socialiteproviders/tiktok": "^5.2",
        "socialiteproviders/twitch": "^5.4",
        "socialiteproviders/twitter": "*",
        "socialiteproviders/youtube": "^4.1",
        "spatie/eloquent-sortable": "^4.4",
        "spatie/error-solutions": "^1.1",
        "spatie/laravel-activitylog": "^4.10",
        "spatie/laravel-data": "^4.11",
        "spatie/laravel-deleted-models": "^1.0",
        "spatie/laravel-enum": "^3.2",
        "spatie/laravel-honeypot": "^4.5",
        "spatie/laravel-ignition": "^2.9",
        "spatie/laravel-medialibrary": "^11.12",
        "spatie/laravel-navigation": "^1.3",
        "spatie/laravel-newsletter": "^5.3",
        "spatie/laravel-permission": "^6.15",
        "spatie/laravel-searchable": "^1.13",
        "spatie/laravel-settings": "^3.4",
        "spatie/laravel-sitemap": "^7.3.6",
        "spatie/laravel-sluggable": "^3.7",
        "spatie/laravel-tags": "^4.9",
        "stephenjude/filament-blog": "^4.0",
        "stephenjude/filament-debugger": "^4.0",
        "stephenjude/filament-feature-flags": "^3.0",
        "stephenjude/filament-jetstream": "^0.0.16",
        "tapp/filament-auditing": "^3.0",
        "team-reflex/discord-php": "^10.4",
        "tomatophp/filament-media-manager": "^1.1",
        "tomatophp/filament-menus": "^2.0",
        "tomatophp/filament-seo": "^1.0.5",
        "z3d0x/filament-fabricator": "^2.4"
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "barryvdh/laravel-debugbar": "^3.15",
        "barryvdh/laravel-ide-helper": "^3.5",
        "fakerphp/faker": "^1.23",
        "larastan/larastan": "^3.0",
        "laravel/pail": "^1.2.2",
        "laravel/pint": "^1.13",
        "laravel/sail": "^1.41",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.6",
        "pestphp/pest": "^3.7",
        "phpunit/phpunit": "^11.5.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": {
        "tomatophp/filament-seo": {
            "type": "vcs",
            "url": "https://github.com/crimsonstrife/filament-seo"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi",
            "@php artisan filament:upgrade",
            "@php artisan telescope:publish"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi",
            "@php -r \"file_exists('database/database.sqlite') || touch('database/database.sqlite');\"",
            "@php artisan migrate --graceful --ansi"
        ],
        "dev": [
            "Composer\\Config::disableProcessTimeout",
            "npx concurrently -c \"#93c5fd,#c4b5fd,#fb7185,#fdba74\" \"php artisan serve\" \"php artisan queue:listen --tries=1\" \"php artisan pail --timeout=0\" \"npm run dev\" --names=server,queue,logs,vite"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
